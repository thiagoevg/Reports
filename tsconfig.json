{
	"compilerOptions": {
		"module": "commonjs",
		"declaration": true,
		"removeComments": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"allowSyntheticDefaultImports": true,
		"target": "es2019",
		"sourceMap": true,
		"outDir": "./dist",
		"baseUrl": "./",
		"incremental": true,
		"esModuleInterop": true,
		"paths": {
			"@tagmedev/tagme-nest-models": ["libs/tagme-nest-models/src"],
			"@tagmedev/tagme-nest-models/*": ["libs/tagme-nest-models/src/*"]
		}
	}
}
